{
    "rules": {
        "prefer-destructuring":"off",
        "no-param-reassign":"off",
        "prefer-const":"error",
        "no-console":0,
        "indent": [
            2,
            2,
            {"SwitchCase": 1}
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb-base"
}